INSERT INTO `email_templates` VALUES (1,1,'PhotosReady1','ed39f93a53','Mr. ZangZing','misterzz@zangzing.com','<%=@album.short_email%>','Your photos are ready','email.photosready','<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">\n<html xmlns=\"http://www.w3.org/1999/xhtml\">\n<head>\n<meta http-equiv=\"Content-Type\" content=\"text/html; charset=UTF-8\">\n<title>*|MC:SUBJECT|*</title>\n\n<style type=\"text/css\">\n		#outlook a{\n			padding:0;\n		}\n		body{\n			width:100% !important;\n		}\n		body{\n			-webkit-text-size-adjust:none;\n		}\n		body{\n			margin:0;\n			padding:0;\n			background:#eeeff1;\n		}\n		img{\n			display:block;\n			border:0;\n			outline:none;\n			text-decoration:none;\n		}\n		p{\n			font-size:16px;\n			margin:0 0 15px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#6a6a6c;\n			line-height:24px;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		p.smaller{\n			font-size:12px;\n			margin:0 0 15px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#6a6a6c;\n			line-height:18px;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		a{\n			color:#336699;\n			font-size:16px;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		a.smaller{\n			color:#336699;\n			font-size:12px;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		h1{\n			font-size:20px;\n			margin:4px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#BCBCBF;\n			font-weight:normal;\n			text-align:right;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		h2{\n			font-size:20px;\n			margin:4px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#2c2c2c;\n			font-weight:normal;\n			text-align:center;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		h3{\n			font-size:18px;\n			margin:0 0 8px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#2c2c2c;\n			font-weight:normal;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		h3.name{\n			font-size:24px;\n			margin:0 0 8px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#2c2c2c;\n			font-weight:normal;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		p.copyright{\n			font-size:12px;\n			margin:0 0 15px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#bcbcbf;\n			line-height:22px;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		p.ftext{\n			font-size:12px;\n			margin:0 0 15px 0;\n			padding:0;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			color:#999999;\n			line-height:22px;\n			text-shadow:1px 1px #FFFFFF;\n		}\n		a.flink{\n			color:#999999;\n			font-size:12px;\n			font-family:\"Helvetica Neue\", Arial, Helvetica, sans-serif;\n			text-shadow:1px 1px #FFFFFF;\n		}\n</style></head>\n<body style=\"-webkit-text-size-adjust: none;margin: 0;padding: 0;background: #eeeff1;width: 100% !important;\">\n\n<table width=\"100%\" border=\"0\" cellspacing=\"0\" cellpadding=\"30\" bgcolor=\"#eeeff1\">\n  <tr>\n    <td width=\"100%\" align=\"center\" valign=\"top\" bgcolor=\"#eeeff1\"><table width=\"630\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\" bgcolor=\"#FFFFFF\">\n      <tr>\n        <td width=\"630\" bgcolor=\"#FFFFFF\"><table width=\"630\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\n          <tr>\n            <td width=\"15\" valign=\"top\"><img src=\"http://static.zangzing.com/email/space.gif\" width=\"15\" height=\"15\" style=\"display: block;border: 0;outline: none;text-decoration: none;\"></td>\n            <td width=\"127\" valign=\"top\"><img src=\"http://static.zangzing.com/email/space.gif\" alt=\"\" width=\"127\" height=\"15\" style=\"display: block;border: 0;outline: none;text-decoration: none;\"></td>\n            <td width=\"277\" valign=\"top\">