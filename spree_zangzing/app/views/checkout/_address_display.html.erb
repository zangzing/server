<%# Caller must provide address and label%>
<table>
      <tr><b><%=label%></b></td></tr>
      <tr><td><b><%= address.full_name%><b></td></tr>
      <tr><td><%= address.address1%></td></tr>
      <% if address.address2 && address.address2.length > 0 %>
            <tr><td><%= address.address2%></td></tr>
      <%end%>
      <tr><td><%= "#{address.city} #{address.state_text} #{address.zipcode}"%></td></tr>
      <tr><td><%= number_to_phone address.phone%></td></tr>
</table>