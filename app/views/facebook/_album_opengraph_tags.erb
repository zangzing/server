

<meta property="og:title" content="<%= "#{@album.name} by #{@album.user.name}" %>"/>
<meta property="og:type" content="website"/>
<meta property="og:image" content="<%= @album.cover ? @album.cover.thumb_url : ''%>"/>
<meta property="og:site_name" content="<%=FACEBOOK_API_KEYS[:og_site_name]%>"/>
<meta property="fb:app_id" content="<%=FACEBOOK_API_KEYS[:app_id]%>" />
<meta property="og:description" content="<%= SystemSetting[:facebook_post_description] %>"/>

