

<meta property="og:title" content="<%= "#{@photo.caption} by #{@photo.user.name}" %>"/>
<meta property="og:type" content="website"/>
<%if @photo.album.private? %>
    <meta property="og:image" content="<%= "http://#{Server::Application.config.application_host}/images/private-photo.png" %>"/>
<%else%>
     <meta property="og:image" content="<%= @photo.thumb_url %>"/>
<%end%>
<meta property="og:site_name" content="<%=FACEBOOK_API_KEYS[:og_site_name]%>"/>
<meta property="fb:app_id" content="<%=FACEBOOK_API_KEYS[:app_id]%>" />
<meta property="og:description" content="<%= SystemSetting[:facebook_post_description] %>"/>
